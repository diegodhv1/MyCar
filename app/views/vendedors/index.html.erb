<h1>Listing vendedors</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Apellido</th>
      <th>Cedula</th>
      <th>Contrase√±a</th>
      <th>Celular</th>
      <th>Telefono</th>
      <th>Correo</th>
      <th>Departamento</th>
      <th>Municipio</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @vendedors.each do |vendedor| %>
      <tr>
        <td><%= vendedor.nombre %></td>
        <td><%= vendedor.apellido %></td>
        <td><%= vendedor.cedula %></td>
        <td><%= vendedor.password%></td>
        <td><%= vendedor.celular %></td>
        <td><%= vendedor.telefono %></td>
        <td><%= vendedor.correo %></td>
        <td><%= vendedor.departamento %></td>
        <td><%= vendedor.municipio %></td>
        <td><%= link_to 'Show', vendedor %></td>
        <td><%= link_to 'Edit', edit_vendedor_path(vendedor) %></td>
        <td><%= link_to 'Destroy', vendedor, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

